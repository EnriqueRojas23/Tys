<h2>Monitoreo de Vehiculos</h2>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class=" col-md-12">
        <div class="row">
            <div class="col-lg-4">
                <div class="ibox float-e-margins">
                    <div class="ibox-content">
                        <div class="form-group">
                            <div class="row progress-stats">
                                <div class="col-sm-9">
                                    <h5 class="name">Vehículos Inspeccionados</h5>
                                    <p class="description deemphasize">Porcentaje de Inspeccionados</p>
                                    <div class="progress progress-sm js-progress-animate bg-aqua">
                                        <div class="progress-bar" role="progressbar" aria-valuenow="10" aria-valuemin="0"
                                             data-width="10%"
                                             aria-valuemax="100" style="width: 20%;">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-3 text-align-center">
                                    <span class="status rounded rounded-lg bg-body-light">
                                        <small><span id="percent-1">20</span>%</small>
                                    </span>
                                </div>
                            </div>
                            @foreach (var item in @Model.vehiculos)
                            {
                                if (!@item.idestado.Equals(2))
                                { continue; }
                                <div class="row">
                                    <section class="widget widget-card bg-primary text-white">
                                        <div class="widget-body clearfix">
                                            <div class="row">
                                                
                                                    <div class="col-md-2">
                                                        <span class="widget-icon">
                                                            <i class="glyphicon glyphicon-globe"></i>
                                                        </span>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <h6 class="no-margin"></h6>
                                                        <p class="h2 no-margin fw-normal">@item.placa </p>
                                                    </div>
                                                    <div class="col-md-4">
                                                    </div>
                                                    <div class="col-md-2">
                                                        <div class="btn-group">
                                                            <a class="btn btn-sm btn-secondary dropdown-toggle" href="#" data-toggle="dropdown" aria-expanded="false">
                                                                <i class="fa fa-angle-down "></i>
                                                            </a>
                                                            <ul class="dropdown-menu">
                                                                <li><a class="dropdown-item"  id="idorden_@item.idvehiculo"  onclick="verorden(@item.idvehiculo);" href="#">Ver Ordenes</a></li>
                                                                <li><a class="dropdown-item" id="forward" href="#">Ver Manifiestos</a></li>
                                                                <li class="dropdown-divider"></li>
                                                                <li><a class="dropdown-item" id="mark-as-read" href="#">Imprimir Manifiestos</a></li>
                                                            </ul>
                                                        </div>
                                                    </div>
                                                </div>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <h6 class="no-margin">Estado</h6>
                                                    <p class="value5">@item.estado</p>
                                                </div>
                                                <div class="col-md-4 fs-sm">
                                                    <h6 class="no-margin">Chofer</h6>
                                                    <p class="value5">@item.chofer</p>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <h6 class="no-margin">Peso</h6>
                                                    <p class="value5">@item.peso</p>
                                                </div>
                                                <div class="col-md-4 fs-sm">
                                                    <h6 class="no-margin">Volumen</h6>
                                                    <p class="value5">@item.volumen</p>
                                                </div>
                                            </div>
                                        </div>
                                    </section>
                                </div>
                            }
                    


                        </div>

                    </div>

                </div>

            </div>


  
