@model Web.TYS.Areas.Seguimiento.Models.Seguimiento.valijaModel

@section Styles {
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/plugins/jqGrid/ui.jqgrid.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/jqgrid.extension.css")" />

}

    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        <i class="fa fa-users fa-4x"></i>
        <h4 class="modal-title">Anular Manifiesto</h4>
        <small class="font-bold">Seleccionar los datos necesarios para el registro.</small>
    </div>
    <div class="modal-body">
        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                   
                        @*<span class="input-group-addon"><i class="fa fa-barcode"></i></span>*@
                        <div class="col-md-6">
                            @Html.TextBox("txtmanifiesto", "", new { @class = "form-control", @required = "true", @index = "1", @placeholder = "Número de manifiesto" })
                        </div>
                            <div class="col-md-6">
                                <button id="btnAnularManifiesto" type="button" class="btn  btn-outline btn-default btn-sm"><i class="fa fa-search"></i>  Buscar</button>
                            </div>
                   
                </div>
                    <div class="form-group">
                        <div class="col-md-12 jqGrid_wrapper">
                            <table id="tblDetallePU" class="table table-striped table-bordered table-hover dataTables-tblDetallePU" width="100%" data-url="@Url.Action("JsonGetListarDespachoDetalle", "Seguimiento")"></table>
                        </div>
                    </div>
            </div>
           
        </div>
    </div>
    <div class="modal-footer">
            <button id="btnAnularManifiesto" type="button"   class="btn  btn-outline btn-default btn-sm"><i class="fa fa-check"></i>  Anular</button>
            <button type="button" class="btn  btn-outline btn-danger btn-sm" data-dismiss="modal"><i class="fa fa-times-circle-o"></i>  Cerrar</button>
      </div>
