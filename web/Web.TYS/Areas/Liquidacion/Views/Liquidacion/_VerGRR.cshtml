@model Web.TYS.Areas.Liquidacion.Models.LiquidacionModel

@section Styles {
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/plugins/jqGrid/ui.jqgrid.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/jqgrid.extension.css")" />

}

    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        @*<i class="fa fa-users fa-4x"></i>*@
        <h4 class="modal-title">Listado de GRR</h4>
        <small class="font-bold">Muestra el detalle de la OT.</small>
    </div>
    <div class="modal-body">
        <div class="row">
            <div class="col-md-12">
                <section class="widget widget-chart-changes">
                    <div class="widget-body">
                        <div class="chart bg-primary-light btlr btrr">
                            <p class="chart-value"><i class="fa fa-caret-up"></i> @Model.numcp</p>
                            <p class="chart-value-change">@Model.numcp</p>
                        </div>

                        <h4 class="chart-title"><span class="fw-normal">@Model.grr</span></h4>
                        <p class="deemphasize">Guias</p>


                    
                
                    </div>
                </section>

            </div>

        </div>

        </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-inverse" data-dismiss="modal">Cerrar</button>
            </div>

