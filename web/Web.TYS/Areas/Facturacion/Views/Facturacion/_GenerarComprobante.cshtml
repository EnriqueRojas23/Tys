@model Web.TYS.Areas.Facturacion.Models.ComprobanteModel

    
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
    <i class="fa fa-newspaper-o fa-4x"></i>
    <h4 class="modal-title">Generar Comprobante de Pago</h4>
    <small class="font-bold">Seleccione el tipo de comprobante de pago que desea generar.</small>
</div>


<div class="modal-footer">
    <div class="form-horizontal">
        <div class="form-group">
            <p class="text-align-center">   
                <div class="col-md-4">
                    <button id="btnGenFactura"  class="btn btn-inverse btn-lg btn-block  width-100 mb-xs" role="button">
                        <i class="fa fa-exclamation text-warning"></i>
                        Factura
                    </button>
                </div>
                <div class="col-md-4">
                    <button id="btnGenBoleta" class="btn btn-inverse btn-lg  btn-block width-100 mb-xs" role="button">
                        <i class="fa-exclamation text-info"></i>
                        <span class="text-info">Boleta</span>

                    </button>
                </div>
                <div class="col-md-4">
                    <button class="btn btn-danger btn-lg  btn-block width-100 mb-xs" data-dismiss="modal" role="button">
                        <i class="fa fa-times-circle-o"></i>
                        Cancelar
                    </button>
                </div>
            </p>
            
        </div>
      
    </div>
    @Html.HiddenFor(x => x.idpreliquidacion)
</div>

