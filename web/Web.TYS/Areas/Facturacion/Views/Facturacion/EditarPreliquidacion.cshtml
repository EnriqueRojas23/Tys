@model Web.TYS.Areas.Facturacion.Models.PreliquidacionModel
@{
    ViewBag.Title = "Pre - Liquidacion";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Styles
{
<link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/plugins/jqGrid/ui.jqgrid.css")" />
<link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/jqgrid.extension.css")" />
<link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/plugins/datapicker/datepicker3.css")" />
<link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/plugins/jasny/jasny-bootstrap.min.css")" />
<link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/plugins/sweetalert/sweetalert.css")" />
<link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/Content/plugins/awesome-bootstrap-checkbox/awesome-bootstrap-checkbox.css")" />
<link rel="stylesheet" type="text/css" media="screen" href="@Url.Content("~/Content/plugins/touchspin/jquery.bootstrap-touchspin.min.css")" />
<link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/plugins/chosen/chosen.css")" />
<link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/plugins/dualListBox/bootstrap-duallistbox.css")" />
<link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/application.css")" />


}   
@section scripts
{
        <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jsview/facturacion/editarpreliquidacion.index.js")" type="text/javascript"></script>    
        <script src="@Url.Content("~/Scripts/jsview/facturacion/editarpreliquidacion.grids.js")" type="text/javascript"></script>   
        <script src="@Url.Content("~/Scripts/plugins/jqGrid/jquery.jqGrid.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/plugins/jqGrid/i18n/grid.locale-es.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/base.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/plugins/datapicker/bootstrap-datepicker.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/plugins/jasny/jasny-bootstrap.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/plugins/sweetalert/sweetalert.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/plugins/touchspin/jquery.bootstrap-touchspin.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/plugins/chosen/chosen.jquery.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
  
}
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Editar - Liquidación</h2>
        <ol class="breadcrumb">
            <li>
                <a><strong>Editar Preliquidacion </strong></a> 
            </li>

        </ol>
    </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5><i class="fa fa-info-circle"></i>  Editar PreLiquidación -  @Model.numeropreliquidacion</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <div class="col-md-2">
                                @Html.HiddenFor(x => x.idpreliquidacion)
                                @Html.HiddenFor(x => x.idcliente)
                                <button id='btnAgregar' type="button" class="btn  btn-line btn-block btn-sm btn-danger"><i class="fa fa-plus"></i> Agregar OT  </button>
                            </div>
                            <div class="col-md-2">
                                <button id='btnEliminar' type="button" class="btn  btn-line btn-block btn-sm btn-github"><i class="fa fa-plus"></i> Eliminar OT  </button>
                            </div>
                            <div class="col-md-2">
                                <button id='btnRecargo' type="button" class="btn  btn-line btn-block btn-sm btn-github"><i class="fa fa-plus"></i> Agregar Recargo  </button>
                            </div>
                            <div class="col-md-2">
                                <button id='btnReporte' type="button" class="btn  btn-line btn-block btn-sm btn-github"><i class="fa fa-external-link-square"></i> Ver Reporte  </button>
                            </div>
                            <div class="col-md-2">
                                <button id='btnRegresar' type="button" class="btn  btn-line btn-block btn-sm btn-github"><i class="fa fa-fast-backward "></i> Regresar  </button>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-12 jqGrid_wrapper">
                                <table id="gridpreliquidacion"
                                       data-dataurl="@Url.Action("JsonGetListarPreliquidacion", "Facturacion")"></table>
                                <div id="gridpreliquidacionpager"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5><i class="fa fa-info-circle"></i>  Totalizados -  @Model.numeropreliquidacion</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <div class="col-md-8 col-md-offset-4">
                                <section class="widget widget-chart-changes">
                                    <div class="widget-body">
                                        <div class="chart bg-success btlr btrr">
                                            <p class="chart-value"><i class="fa fa-caret-up"></i> Totalizados</p>
                                        </div>
                                        <div class="mt">
                                            <div class="row">

                                                <div class="col-xs-3 col-xs-offset-3">
                                                    <p class="h4 no-margin" id="cantidadot_label"></p>
                                                    <p class="deemphasize"># OT</p>
                                                </div>
                                                <div class="col-xs-3">
                                                    <p class="h4 no-margin" id="bultos_label"></p>
                                                    <p class="deemphasize">Total Bultos</p>
                                                </div>
                                                <div class="col-xs-3">
                                                    <p class="h4 no-margin" id="recargos_label"></p>
                                                    <p class="deemphasize">Recargos</p>
                                                </div>

                                            </div>
                                        </div>
                                        <div class="mt">
                                            <div class="row">
                                                <div class="col-xs-3 col-xs-offset-3">

                                                </div>
                                                <div class="col-xs-3">
                                                    <p class="h4 no-margin" id="peso_label"></p>
                                                    <p class="deemphasize">Total Peso</p>
                                                </div>


                                                <div class="col-xs-3" style="font-weight:bold">
                                                    <p class="h4 no-margin" id="subtotal_label"></p>
                                                    <p class="deemphasize">SubTotal</p>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="mt">
                                            <div class="row">
                                                <div class="col-xs-3 col-xs-offset-6">
                                                    <p class="h4 no-margin" id="volumen_label"></p>
                                                    <p class="deemphasize">Total Volumen</p>
                                                </div>
                                                <div class="col-xs-3" style="font-weight:bold">
                                                    <p class="h4 no-margin" id="igv_label"></p>
                                                    <p class="deemphasize">IGV</p>
                                                </div>
                                              
                                            </div>
                                        </div>
                                        <div class="mt">
                                            <div class="row">
                                                <div class="col-xs-3 col-xs-offset-9" style="font-weight:bold">
                                                    <p class="h4 no-margin" style="color:red" id="total_label"></p>
                                                    <p class="deemphasize">Total</p>
                                                </div>
                                            </div>
                                        </div>
                                        </div>
</section>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            @Html.HiddenFor(x => x.strtotal)
            @Html.HiddenFor(x => x.strsubtotal)
            @Html.HiddenFor(x => x.strigv)


            @Html.HiddenFor(x=>x.totalpeso)
            @Html.HiddenFor(x => x.totalvolumen)
            @Html.HiddenFor(x => x.totalbulto)
            @Html.HiddenFor(x => x.recargo)
            @Html.HiddenFor(x => x.cantidad)
        </div>
    </div>
</div>

